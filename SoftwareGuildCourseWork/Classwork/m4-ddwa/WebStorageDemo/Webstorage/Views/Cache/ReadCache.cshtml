@model IEnumerable<WebStorage.Models.Contact>

@{
    ViewBag.Title = "Read Cache";
}

<div class="row">
    <div class="col-xs-12">
        <h2>Reading from Cache</h2>
        <p>Here is the data currently in the cache:</p>
        <table>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Email
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Email
                    </td>
                </tr>
            }
        </table>
    </div>
</div>



